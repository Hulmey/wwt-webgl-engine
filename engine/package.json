    "doc": "typedoc src/index.d.ts",
    "lint": "tsc",
    "test": "mocha-headless-chrome -f tests/tests.html -r xunit >tests/results.xml",
    "tscheck": "tsc",
    "uglify": "uglifyjs src/index.js -c -o src/index.min.js"
